function and:
read Op1,Op2
%
    if(not Op1)then
        Result := nil
    else
        Result := (other Op2 Op1 A)
    fi;

    for Op2 do
        Result := (tl Result)
    od;

    foreach X in Op2 do
     Test := ( cons nil X )
    od

%
write Result
